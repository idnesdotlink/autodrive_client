  },
  "main": "./dist/electron/main.js",
  "scripts": {
    "builder": "cross-env DEBUG=electron-builder electron-builder --publish always --config electron-builder.yml",
    "clean": "cross-env node -r @babel/register .compiler/clean.es6",
    "electron:appimage": "npm run clean && npm run icon:create && npm run electron:build && npm run builder && dist/output/*.AppImage",
    "electron:build": "cross-env NODE_ENV=production node -r @babel/register .compiler/electron/build.es6",
    "electron:dev": "cross-env HMR=true node -r @babel/register .compiler/electron/dev.es6",
    "gts:check": "node_modules/.bin/gts check",
    "gts:fix": "node_modules/.bin/gts check",
    "icon:create": "node -r @babel/register .compiler/icon.es6",
    "ngsw:build": "cross-env SW=true npm run web:build && npm run ngsw:config && npm run ngsw:copy",
    "ngsw:config": "node_modules/.bin/ngsw-config dist/web /src/web/ngsw-config.json",
    "ngsw:copy": "cp node_modules/@angular/service-worker/ngsw-worker.js dist/web/",
    "ngsw:serve": "npm run clean && npm run ngsw:build && npm run php",
    "php": "php -S 127.0.0.1:9080 -t ./dist/web",
    "release": "npm run clean && npm run icon:create && npm run electron:build && npm run builder",
    "test": "echo \"Error: no test specified\" && exit 1",
    "web:build": "cross-env NODE_ENV=production WEB=true node -r @babel/register .compiler/web/build.es6",
    "web:dev": "cross-env WEB=true HMR=true node -r @babel/register .compiler/web/dev.es6"
  },
